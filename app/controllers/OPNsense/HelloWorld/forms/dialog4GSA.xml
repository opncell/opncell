
<form>
    <field>
        <id>4G-Standalone.generalFourGSA.Enabled</id>
        <label>enabled</label>
        <type>checkbox</type>
        <help>Enable this feature</help>
    </field>
    <field>
        <id>4G-Standalone.generalFourGSA.SMTPHost</id>
        <label>SMTPHost</label>
        <type>text</type>
        <help><![CDATA[ip address of the mail host]]></help>
        <hint>choose a valid IPv4/v6 address</hint>
    </field>
    <field>
        <id>4G-Standalone.generalFourGSA.FromEmail</id>
        <label>Email (from)</label>
        <type>text</type>
    </field>
    <field>
        <id>4G-Standalone.generalFourGSA.ToEmail</id>
        <label>Email (to)</label>
        <type>text</type>
        <help>This is email info</help>
    </field>
    <field>
        <id>4G-Standalone.generalFourGSA.Description</id>
        <label>Description</label>
        <type>text</type>
        <help>This is email info</help>
    </field>
    <field>
        <id>4G-Standalone.generalFourGSA.Interface</id>
        <label>Interface</label>
        <type>text</type>
    </field>
</form>

